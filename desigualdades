%%inecuasion de primer grado
%autores ORLANDO MENDOZA VALENTIN ,DIEGO CRUZ GARDUÑO
syms x
disp ('  inecuasiones de primer grado  ')
disp ('x-8 < 15 ');
solve (x-8 < 15)
x=-20:1:20;
y=x-20;
subplot(2,2,1);
plot(x,y)
xlabel('x');
ylabel('-∞');
title ('(-∞,20)')
%inecuasion de segundo grado
syms x
disp ('  inecuasiones de segundo grado  ')
disp ('x^2 + 4x < 3 ');
solve (x^2 + 4*x < 3)
x=4.5:1:3;
y=x=4.5;
subplot(2,2,2);
plot(x,y)
xlabel('x');
ylabel('-∞');
title ('(-3,4.5)')
